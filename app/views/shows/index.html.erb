<h1>Listing shows</h1>
 <%= link_to 'New show', new_show_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Venue</th>
    <th colspan="2"></th>
  </tr>
 
  <% @shows.each do |show| %>
    <tr>
      <td><%= show.name %></td>
      <td><%= show.venue %></td>
      <td><%= link_to 'Show', show_path(show) %></td>
      <td><%= link_to 'Edit', edit_show_path(show) %></td>
    </tr>
  <% end %>
</table>